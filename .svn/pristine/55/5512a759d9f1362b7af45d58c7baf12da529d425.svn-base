package tts.moudle.api.bean;

/**
 * Created by sjb on 2016/2/26.
 */
public class BarBean {
    private String msg;
    private boolean IsRemoveBack;
    private int colorid;//toolbar背景色
    private String subTitle;//副标题
    private int iconId;//图标
    private String iconName;
    private int textColorid;//标题字体颜色
    private int subTextColorid;//副标题字体颜色


    public int getTextColorid() {
        return textColorid;
    }

    public void setTextColorid(int textColorid) {
        this.textColorid = textColorid;
    }

    public int getSubTextColorid() {
        return subTextColorid;
    }

    public void setSubTextColorid(int subTextColorid) {
        this.subTextColorid = subTextColorid;
    }

    public String getIconName() {
        return iconName;
    }

    public BarBean setIconName(String iconName) {
        this.iconName = iconName;
        return this;
    }

    public int getIconId() {
        return iconId;
    }

    public BarBean setIconId(int iconId) {
        this.iconId = iconId;
        return this;
    }

    public String getSubTitle() {
        return subTitle;
    }

    public BarBean setSubTitle(String subTitle) {
        this.subTitle = subTitle;
        return this;
    }

    public String getMsg() {
        return msg;
    }

    public BarBean setMsg(String msg) {
        this.msg = msg;
        return this;
    }

    public boolean isRemoveBack() {
        return IsRemoveBack;
    }

    public BarBean setIsRemoveBack(boolean isRemoveBack) {
        IsRemoveBack = isRemoveBack;
        return this;
    }

    public int getColorid() {
        return colorid;
    }

    public BarBean setColorid(int colorid) {
        this.colorid = colorid;
        return this;
    }
}
