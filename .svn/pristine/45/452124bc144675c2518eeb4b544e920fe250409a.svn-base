<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_live_over"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#000000"
    >
    <TextView
        android:textSize="@dimen/y40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorPrimary"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/y50"
        android:text="直播已结束"
        />
    <TextView
        android:id="@+id/watch_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="@dimen/y30"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/y120"
        android:layout_gravity="center_horizontal"
        />
    <TextView
        android:textSize="@dimen/y33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="看过人数"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/y40"
        android:layout_gravity="center_horizontal"
        />

    <TextView
        android:textSize="@dimen/y33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="分享到"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/y300"
        android:layout_gravity="center_horizontal"
        />
    <RelativeLayout
        android:layout_marginTop="@dimen/y70"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/weixin"
            android:layout_marginLeft="@dimen/x80"
            android:layout_width="@dimen/x90"
            android:layout_height="@dimen/x90"
            android:src="@drawable/weixin"/>
        <ImageView
            android:id="@+id/pengyouquan"
            android:layout_centerHorizontal="true"
            android:layout_width="@dimen/x90"
            android:layout_height="@dimen/x90"
            android:src="@drawable/pengyouquan"/>
        <ImageView
            android:id="@+id/qq"
            android:layout_marginRight="@dimen/x80"
            android:layout_alignParentRight="true"
            android:layout_width="@dimen/x90"
            android:layout_height="@dimen/x90"
            android:src="@drawable/qq"/>
    </RelativeLayout>

    <TextView
        android:id="@+id/guanzhu_wanghong"
        android:layout_marginTop="@dimen/y50"
        android:layout_marginRight="@dimen/x40"
        android:layout_marginLeft="@dimen/x40"
        android:gravity="center"
        android:background="@drawable/live_over_guanzhu"
        android:textColor="@color/colorPrimary"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="关注主播"
        android:layout_gravity="center_horizontal"
        />
    <TextView
        android:id="@+id/back_to_home"
        android:layout_marginTop="@dimen/y30"
        android:layout_marginRight="@dimen/x40"
        android:layout_marginLeft="@dimen/x40"
        android:gravity="center"
        android:background="@drawable/live_over_guanzhu"
        android:textColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="返回首页"
        android:layout_gravity="center_horizontal"
        />
</LinearLayout>
