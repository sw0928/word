<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_selected="true">
        <shape
            android:shape="oval"
            android:useLevel="false">

            <solid android:color="#ffffffff"/>
            <size
                android:width="12dp"
                android:height="12dp"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">

            <solid android:color="#ffffffff"/>
            <stroke
                android:width="4dp"
                android:color="#00ffffff"/>
            <size
                android:width="12dp"
                android:height="12dp"/>
        </shape>
    </item>
</selector> 
