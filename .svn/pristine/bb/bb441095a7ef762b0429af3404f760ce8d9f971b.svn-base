<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_others_space"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    android:orientation="vertical"
  >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <include layout="@layout/toolbar_item"/>
    <LinearLayout
        android:background="@drawable/me_bg"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y500"
        android:orientation="vertical">
        <tts.moudle.api.widget.CircleImageView
            android:id="@+id/icon"
            android:layout_marginTop="@dimen/y90"
            android:layout_width="@dimen/x120"
            android:layout_height="@dimen/x120"
            android:layout_gravity="center_horizontal"/>

        <TextView
            android:id="@+id/nickname"
            android:text="134"
            android:layout_marginTop="@dimen/y40"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"/>
        <RelativeLayout
            android:layout_marginTop="@dimen/y70"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x90">
            <TextView
                android:id="@+id/guanzhu_count"
                android:layout_marginRight="@dimen/x50"
                android:layout_toLeftOf="@+id/line"
                android:text="134"
                android:layout_marginTop="@dimen/y20"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"/>
            <View
                android:id="@+id/line"
                android:layout_centerHorizontal="true"
                android:background="@color/white"
                android:layout_width="@dimen/x4"
                android:layout_height="match_parent"/>

            <TextView
                android:id="@+id/fans_count"
                android:layout_marginLeft="@dimen/x50"
                android:layout_toRightOf="@+id/line"
                android:text="134"
                android:layout_marginTop="@dimen/y20"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"/>
        </RelativeLayout>

        <TextView
            android:layout_marginBottom="@dimen/y30"
            android:id="@+id/autograph1"
            android:textSize="@dimen/y40"
            android:text="134"
            android:layout_marginTop="@dimen/y40"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"/>
    </LinearLayout>
    <LinearLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <RelativeLayout
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/x50"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/y90">
        <TextView
            android:textSize="@dimen/y30"
            android:text="年龄"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <TextView
            android:id="@+id/nianling"
            android:layout_marginLeft="@dimen/x200"
            android:textSize="@dimen/y30"
            android:text="年龄"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
    </RelativeLayout>
    <View
        android:layout_marginLeft="@dimen/x50"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x1"
        android:background="@color/black"/>

        <RelativeLayout
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/x50"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/y90">
            <TextView
                android:textSize="@dimen/y30"
                android:text="幽秘号"
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/youmihao"
                android:layout_marginLeft="@dimen/x200"
                android:textSize="@dimen/y30"
                android:text="年龄"
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />
        </RelativeLayout>
        <View
            android:layout_marginLeft="@dimen/x50"

            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:background="@color/black"/>

        <RelativeLayout
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/x50"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/y90">
            <TextView
                android:textSize="@dimen/y30"
                android:text="个性签名"
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/autograph"
                android:layout_marginLeft="@dimen/x200"
                android:textSize="@dimen/y30"
                android:text="年龄"
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />
        </RelativeLayout>
        <View
            android:layout_marginLeft="@dimen/x50"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:background="@color/black"/>
    </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_alignParentBottom="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <View

        android:background="@color/black"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x1"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y80">
        <TextView
            android:id="@+id/guanzhu"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="关注"
            android:textColor="@color/colorPrimary"
            />
        <View
            android:background="@color/black"
            android:layout_marginBottom="@dimen/x10"
            android:layout_marginTop="@dimen/y10"
            android:layout_width="@dimen/x1"
            android:layout_height="match_parent"/>
        <TextView
            android:id="@+id/talk_private"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="私信"
            android:textColor="@color/colorPrimary"
            />
    </LinearLayout>
    </LinearLayout>
</RelativeLayout>
