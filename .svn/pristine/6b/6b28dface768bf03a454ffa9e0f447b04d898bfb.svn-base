<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tst.project.dao.interfaces.IntegralShopDao">

	<select id="getIntegralGoodss" parameterType="goodsBean" resultType="goodsBean">
		select * from tst_goods
		where is_deduct_integral='1' and goods_state='1' and is_delete='0'
	</select> 
	
</mapper>