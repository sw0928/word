package tst.project.bean.order;

public class OrderParameterBean {
	private int order_parameter_id;
	private String order_id;
	private String order_goods_id;
	private String parameter_id;
	private String parameter_price;
	private String parameter_name;
	
	
	public String getParameter_name() {
		return parameter_name;
	}
	public OrderParameterBean setParameter_name(String parameter_name) {
		this.parameter_name = parameter_name;
		return this;
	}
	public int getOrder_parameter_id() {
		return order_parameter_id;
	}
	public OrderParameterBean setOrder_parameter_id(int order_parameter_id) {
		this.order_parameter_id = order_parameter_id;
		return this;
	}
	public String getOrder_id() {
		return order_id;
	}
	public OrderParameterBean setOrder_id(String order_id) {
		this.order_id = order_id;
		return this;
	}
	
	public String getOrder_goods_id() {
		return order_goods_id;
	}
	public OrderParameterBean setOrder_goods_id(String order_goods_id) {
		this.order_goods_id = order_goods_id;
		return this;
	}
	public String getParameter_id() {
		return parameter_id;
	}
	public OrderParameterBean setParameter_id(String parameter_id) {
		this.parameter_id = parameter_id;
		return this;
	}
	public String getParameter_price() {
		return parameter_price==null||parameter_price.equals("")?"0":parameter_price;
	}
	public OrderParameterBean setParameter_price(String parameter_price) {
		this.parameter_price = parameter_price;
		return this;
	}
}
