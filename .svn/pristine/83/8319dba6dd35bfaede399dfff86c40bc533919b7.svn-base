$base: 'react-date-picker__history-view';

.#{$base}--theme-#{$THEME_NAME} {
  border: 1px solid gray;
  padding: 2px;

  .react-date-picker__year-view--theme-#{$THEME_NAME},
  .react-date-picker__decade-view--theme-#{$THEME_NAME} {
    border: none;
  }
}

.react-date-picker__nav-bar .#{$base}--theme-#{$THEME_NAME} {
  font-size: 0.833em;
}
