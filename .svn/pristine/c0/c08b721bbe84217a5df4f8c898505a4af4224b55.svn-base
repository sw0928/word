$background-color: #f0f0f0;
$border-color: #aeaeae;
$selected-color: #2e99eb;
$selected-range-color: #6eb8f1;
$muted-color: #aeaeae;
$text-color: #000;

$dp-datepicker-height: 100%;
$dp-datepicker-width: 100%;
$dp-datepicker-border-width: 1px;
$dp-datepicker-border-style: solid;
$dp-datepicker-border-color: $border-color;
$dp-datepicker-border-radius: 4px;
$dp-datepicker-header-background: $background-color;
$dp-datepicker-header-color: $text-color;
$dp-datepicker-header-over-background: inherit;
$dp-datepicker-header-padding: 8px;
$dp-datepicker-week-day-names-border-width: 1px;
$dp-datepicker-week-day-names-border-style: solid;
$dp-datepicker-week-day-names-border-color: $border-color;
$dp-datepicker-week-day-names-background: $dp-datepicker-header-background;
$dp-datepicker-week-day-names-color: $dp-datepicker-header-color;
$dp-datepicker-week-day-name-border: none;
$dp-datepicker-day-color: inherit;
$dp-datepicker-day-bg: inherit;
$dp-datepicker-day-margin: 2px;
$dp-datepicker-day-border-radius: 4px;
$dp-datepicker-today-font-weight: bold;
$dp-datepicker-day-other-month-color: #5c5c5c; // darken(#999, 40%)
$dp-datepicker-day-disabled-color: $muted-color;
$dp-datepicker-day-other-month-bg: inherit;
$dp-datepicker-day-disabled-bg: $dp-datepicker-day-other-month-bg;
$dp-datepicker-day-in-range-bg: #E2F0FF;
$dp-datepicker-font-size: 14px;
$dp-datepicker-month-name-bg: inherit;
$dp-datepicker-month-name-over-bg: $background-color;
$dp-datepicker-nav-bg: $muted-color;
$dp-datepicker-nav-over-bg: darken($muted-color, 10%);
$dp-datepicker-row-border-width: 0;
$dp-datepicker-row-border-style: solid;
$dp-datepicker-row-border-color: transparent;
$dp-datepicker-value-color: #fff;
$dp-datepicker-value-font-weight: normal;
$dp-datepicker-value-bg: $selected-color;
$dp-datepicker-value-selected-range-bg: $selected-range-color;
$dp-datepicker-cell-border-width: 0;
$dp-datepicker-cell-border-style: solid;
$dp-datepicker-cell-border-color: transparent;
$dp-datepicker-cell-over-background: $background-color;
$dp-datepicker-cell-over-font-weight: inherit;
$dp-datepicker-cell-over-color: inherit;
$dp-datepicker-cell-padding: 0;
$dp-datepicker-header-cell-border: 10px solid transparent;
$dp-datepicker-weekend-highlight-color: #BF0909;
$dp-datepicker-weekend-highlight-value-color: #EFE3E3;

