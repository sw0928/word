package tst.project.bean.order;

public class OrderServiceBean {
	private int order_service_id;
	private String order_id;
	private String order_goods_id;
	private String service_id;
	private String service_price;
	private String service_name;
	

	public String getService_name() {
		return service_name;
	}
	public OrderServiceBean setService_name(String service_name) {
		this.service_name = service_name;
		return this;
	}
	public int getOrder_service_id() {
		return order_service_id;
	}
	public OrderServiceBean setOrder_service_id(int order_service_id) {
		this.order_service_id = order_service_id;
		return this;
	}
	public String getOrder_id() {
		return order_id;
	}
	public OrderServiceBean setOrder_id(String order_id) {
		this.order_id = order_id;
		return this;
	}
	
	public String getOrder_goods_id() {
		return order_goods_id;
	}
	public OrderServiceBean setOrder_goods_id(String order_goods_id) {
		this.order_goods_id = order_goods_id;
		return this;
	}
	public String getService_id() {
		return service_id;
	}
	public OrderServiceBean setService_id(String service_id) {
		this.service_id = service_id;
		return this;
	}
	public String getService_price() {
		return service_price==null||service_price.equals("")?"0":service_price;
	}
	public OrderServiceBean setService_price(String service_price) {
		this.service_price = service_price;
		return this;
	}
}
