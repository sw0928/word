<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    android:background="@color/common_bg"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:gravity="center"
            android:textSize="@dimen/y32"
            android:textColor="@color/white"
            android:layout_height="@dimen/y90"
            android:text="我的"/>
        <RelativeLayout
            android:background="@drawable/me_bg"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y288">
            <tts.moudle.api.widget.CircleImageView
                android:id="@+id/me_fragment_icon"
                android:layout_width="@dimen/x110"
                android:layout_height="@dimen/x110"
                android:layout_marginTop="@dimen/y20"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:id="@+id/me_fragment_nickname"
                android:text="印象东方 ID:1314"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/y26"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/y150"/>
            <TextView
                android:id="@+id/me_fragment_count_piao"
                android:text="印象东方 ID:1314"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/y26"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/y200"/>
            <TextView
                android:layout_marginRight="@dimen/x10"
                android:id="@+id/me_fragment_details"
                android:textColor="@color/white"
                android:layout_marginTop="@dimen/y80"
                android:layout_alignParentRight="true"
                android:textSize="@dimen/y24"
                android:text="详细信息 >"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/y90"
            android:background="@color/white">
            <TextView
                android:gravity="center_vertical"
                android:id="@+id/me_fragment_my_guanzhu"
                android:text="我的关注"
                android:layout_marginLeft="@dimen/x100"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#323232"
                android:textSize="@dimen/y28"
                />
            <TextView
                android:gravity="center_vertical"
                android:id="@+id/me_fragment_my_fans"
                android:text="我的粉丝"
                android:layout_marginRight="@dimen/x100"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#323232"
                android:textSize="@dimen/y28"
                />

        </RelativeLayout>
        <LinearLayout
            android:background="#f5f5f5"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y20" />
        <LinearLayout
            android:showDividers="middle"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y240"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/me_fragment_rl_my_dimends"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y80">
                <TextView
                    android:id="@+id/myzuanshi"
                    android:text="我的秘豆"
                    android:layout_marginLeft="@dimen/x20"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#323232"
                    android:textSize="@dimen/y28"
                    />
                <LinearLayout
                    android:layout_marginRight="@dimen/x20"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:drawablePadding="@dimen/x20"
                        android:drawableLeft="@drawable/beans"
                        android:id="@+id/me_fragment_dimend_count"
                        android:layout_marginLeft="@dimen/x5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="10"
                        android:textSize="@dimen/y28"
                        android:textColor="@color/RGB_909090"
                        />
                    <ImageView
                        android:visibility="gone"

                        android:layout_marginLeft="@dimen/x5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/tiaozhuan"/>
                </LinearLayout>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/x1"
                android:background="#f5f5f5"></LinearLayout>

            <RelativeLayout
                android:id="@+id/me_fragment_rl_my_u"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y80">
                <TextView
                    android:id="@+id/myxuanpiao"
                    android:text="秘宝"
                    android:layout_marginLeft="@dimen/x20"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#323232"
                    android:textSize="@dimen/y28"
                    />
                <TextView
                    android:id="@+id/u_coid_count"
                    android:layout_marginRight="@dimen/x50"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:text="0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/RGB_909090"
                    />
                <ImageView
                    android:visibility="gone"

                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/x20"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/tiaozhuan"/>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/x1"
                android:background="@color/common_bg"/>
            <RelativeLayout
                android:id="@+id/me_Fragment_my_video"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y80">
                <TextView
                    android:text="我的视频"
                    android:layout_marginLeft="@dimen/x20"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#323232"
                    android:textSize="@dimen/y28"
                    />
                <TextView
                    android:layout_marginRight="@dimen/x50"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:text=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/RGB_909090"
                    />
                <ImageView
                    android:visibility="gone"

                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/x20"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/tiaozhuan"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/x1"
                android:background="#f5f5f5"></LinearLayout>
            <RelativeLayout
                android:visibility="gone"
                android:id="@+id/me_fragment_renzheng"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y80">
                <TextView
                    android:id="@+id/my52bi"
                    android:text="网红认证"
                    android:layout_marginLeft="@dimen/x20"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#323232"
                    android:textSize="@dimen/y28"
                    />
                <TextView
                    android:id="@+id/state"
                    android:layout_marginRight="@dimen/x50"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:text="认证中"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/RGB_909090"
                    />
                <ImageView
                    android:visibility="gone"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/x20"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/tiaozhuan"/>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:background="#f5f5f5"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y20" />

        <RelativeLayout
            android:id="@+id/rl_setting"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y80">
            <TextView
                android:text="退出"
                android:layout_marginLeft="@dimen/x20"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#323232"
                android:textSize="@dimen/y28"
                />
            <TextView
                android:layout_marginRight="@dimen/x50"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/RGB_909090"
                />
            <ImageView
                android:visibility="gone"

                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x20"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/tiaozhuan"/>
        </RelativeLayout>
        <View
            android:layout_marginRight="@dimen/x30"
            android:layout_marginLeft="@dimen/x30"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x1"
            android:background="@color/common_bg"/>
    </LinearLayout>
</ScrollView>